{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "drupal/address": "^1.10",
        "drupal/address_map_link": "^1.3",
        "drupal/addtoany": "^1.16",
        "drupal/addtocalendar": "^3.2",
        "drupal/admin_toolbar": "^3.1",
        "drupal/algolia_module_custom": "^2.0",
        "drupal/allowed_formats": "^1.5",
        "drupal/anchor_link": "^2.5",
        "drupal/basic_watermark": "^2.0",
        "drupal/better_exposed_filters": "^5.1",
        "drupal/bigmenu": "^2.0@RC",
        "drupal/blazy": "^2.13",
        "drupal/block_aria_landmark_roles": "^1.4",
        "drupal/block_class": "^2.0",
        "drupal/block_field": "^1.0@RC",
        "drupal/bootstrap5": "^1.1",
        "drupal/bootstrap_select": "^1.0",
        "drupal/calendar_link": "^2.1",
        "drupal/charts": "^5.0",
        "drupal/charts_twig": "^1.0",
        "drupal/chosen": "^3.0",
        "drupal/ckeditor_entity_link": "^1.2",
        "drupal/clientside_validation": "^3.0",
        "drupal/colorbutton": "^1.3",
        "drupal/colordialog": "^1.3",
        "drupal/colossal_menu": "^2.1",
        "drupal/core-composer-scaffold": "^9.4",
        "drupal/core-project-message": "^9.4",
        "drupal/core-recommended": "^9.4",
        "drupal/countries_info": "^1.2",
        "drupal/country_state_city": "^2.1",
        "drupal/csv_serialization": "^2.1",
        "drupal/ctools": "^3.7",
        "drupal/datalayer": "^1.0@beta",
        "drupal/devel": "^4.1",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/editor_advanced_link": "^2.0",
        "drupal/embed": "^1.5",
        "drupal/entity_browser": "^2.7",
        "drupal/entity_clone": "^1.0@beta",
        "drupal/entity_embed": "^1.2",
        "drupal/entity_reference_revisions": "^1.9",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/facets": "^2.0",
        "drupal/fakeobjects": "^1.1",
        "drupal/field_group": "^3.2",
        "drupal/flag": "^4.0@beta",
        "drupal/format_bytes": "^2.0",
        "drupal/geofield": "^1.40",
        "drupal/geolocation": "^3.10",
        "drupal/google_tag": "^1.5",
        "drupal/image_url_formatter": "^1.0",
        "drupal/jquery_ui": "^1.4",
        "drupal/jquery_ui_datepicker": "^1.2",
        "drupal/jquery_ui_slider": "^1.1",
        "drupal/jquery_ui_touch_punch": "^1.0",
        "drupal/like_dislike": "^2.1",
        "drupal/link_attributes": "^1.11",
        "drupal/link_class": "^2.0",
        "drupal/media_directories": "^2.0",
        "drupal/menu_block": "^1.8",
        "drupal/menu_breadcrumb": "^1.16",
        "drupal/menu_link": "^2.0",
        "drupal/menu_select": "^2.0@RC",
        "drupal/menu_trail_by_path": "^1.3",
        "drupal/metatag": "^1.19",
        "drupal/mobile_detect": "^3.0",
        "drupal/panelbutton": "^1.4",
        "drupal/paragraph_accordion": "^1.0",
        "drupal/paragraphs": "^1.14",
        "drupal/paragraphs_asymmetric_translation_widgets": "^1.0@beta",
        "drupal/pathauto": "^1.10",
        "drupal/permissions_by_term": "^3.1",
        "drupal/popin": "^2.0",
        "drupal/quick_node_clone": "^1.15",
        "drupal/redirect": "^1.7",
        "drupal/redis": "^1.5",
        "drupal/responsive_menu": "^4.4",
        "drupal/schema_metatag": "^2.3",
        "drupal/search_api": "^1.23",
        "drupal/search_api_algolia": "^3.0@beta",
        "drupal/sitemap": "^2.0",
        "drupal/sitewide_alert": "^2.0",
        "drupal/slick": "^2.6",
        "drupal/smart_date": "^3.5",
        "drupal/social_media_links": "^2.8",
        "drupal/social_sharing": "^1.0@alpha",
        "drupal/styled_google_map": "^2.0",
        "drupal/superfish": "^1.4",
        "drupal/svg_image": "^1.16",
        "drupal/tb_megamenu": "^1.7",
        "drupal/term_reference_tree": "^1.1",
        "drupal/tid_to_name": "^1.1",
        "drupal/title_length": "^1.2",
        "drupal/token": "^1.10",
        "drupal/tvi": "^1.0@RC",
        "drupal/twig_debugger": "^1.1",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^3.1",
        "drupal/twig_vardumper": "^3.0",
        "drupal/views_current_path": "^2.0",
        "drupal/views_data_export": "^1.1",
        "drupal/views_dates": "^1.1",
        "drupal/views_taxonomy_term_name_into_id": "^1.0@alpha",
        "drupal/views_year_filter": "^1.7",
        "drupal/vote_up_down": "^1.0@alpha",
        "drupal/votingapi": "^3.0@beta",
        "drupal/webform": "^6.1",
        "psr/simple-cache": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
